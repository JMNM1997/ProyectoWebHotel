<form method="post" action="{{ path('complemento_delete', {'idcomplemento': complemento.idcomplemento}) }}" onsubmit="return confirm('¿Estás seguro de que deseas borrarlo?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ complemento.idcomplemento) }}">
    <button class="btn">Borrar</button>
</form>
